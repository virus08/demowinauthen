<h1>Alluser</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Display Name</th>
      <th scope="col">mail</th>
      <th scope="col">jobTitle</th>
      <th scope="col">mobilePhone</th>
    </tr>
  </thead>
  <tbody>
    {{#each alluser}}
      <tr>
        <td>{{this.displayName}}</td>
        <td>{{this.mail}}</td>
        <td>{{this.jobTitle}}</td>
        <td>{{this.mobilePhone}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
{{usertxt}}